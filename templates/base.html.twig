<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>
            {% block title %}
                D&J Professionelle PolsterReinigung
            {% endblock %}
        </title>
        <link href="https://fonts.googleapis.com/css2?family=Zen+Tokyo+Zoo&display=swap" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css2?family=Encode+Sans+SC:wght@100&display=swap" rel="stylesheet">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
            <script src="js/jqactions.js"></script>
        {% endblock %}
    </head>
    <body>
        {% block head %}
            <div class="head-container contrainer">
                <span class="logo-text">D&J</span>
                <span class="main-text d-none d-lg-inline-block">Professionelle PolsterReinigung</span>
                <div class="head-nav d-none d-lg-inline-block">
                    <button class="main-button" onclick="location.href='/home'">Home</button>
                    <button class="main-button" onclick="location.href='/services'">Leistungen</button>
                    <button class="main-button" onclick="location.href='/contact'">Kontakt</button>
                </div>
                <i class="head-menu d-inline-block d-lg-none" data-menu="true"></i>
            </div>
            <div class="head-nav custom-nav d-none">
                <button class="main-button" onclick="location.href='/home'">Home</button><br />
                <button class="main-button" onclick="location.href='/services'">Leistungen</button><br />
                <button class="main-button" onclick="location.href='/contact'">Kontakt</button>
            </div>
        {% endblock %}
        {% block body %}

        {% endblock %}
        {% block foot %}
            <div class="foot-container">
                <div class="links-container">
                    <a href="#">Impressum</a> |
                    <a href="#">Kontakt</a>
                </div>
                <span class="main-text">Copyright Â© js.px 2021</span>
                <span class="main-text">Alle Rechte vorbehalten.</span>
                <!--<button class="main-button">Einloggen</button>-->
            </div>
        {% endblock %}
    </body>
</html>
